{
  "expo": {
    "name": "dkt",
    "slug": "dkt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/DKT-Logo.png",
    "scheme": "dkt",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/DKT-Logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.burhanuddin4747.dkt"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/DKT-Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/DKT-Logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3ce542ea-4681-4e52-b8f2-50147002ef31"
      }
    },
    "owner": "burhanuddin4747"
  }
}
